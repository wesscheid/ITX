{
  "name": "instatranscribe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "node server/server.js",
    "start": "node server/server.js",
    "local-start": "concurrently \"npm run server\" \"npm run dev\" --names \"SERVER,CLIENT\" --prefix-colors \"yellow,cyan\"",
    "render-build": "npm install && tsc -b && vite build && cd server && npm install && chmod +x download-ytdlp.sh && ./download-ytdlp.sh"
  },
  "dependencies": {
    "@google/genai": "^1.33.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "tailwindcss": "^3.4.19",
    "autoprefixer": "^10.4.23",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
